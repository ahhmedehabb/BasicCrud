{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=Products;Integrated Security=True;"
  },
  "Attachments": {
    "AllowedExtensions": "jpg;jpeg;bmp;png",
    "MaxSizeInMegaBytes": 50,
    "EnableCompression": true
  }
}
